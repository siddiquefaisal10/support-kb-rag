# Server
PORT=4000

# MongoDB
MONGO_URI=mongodb://username:password@host:port/database?authSource=admin

# Model Configuration
DEFAULT_MODEL=groq
ENABLED_MODELS=mock,gemini,groq

# Google Gemini
